<h1>All Lists</h1>


  <% @lists.each do |list| %>
	<div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/breakfast.jpg)">
  		<%= list.name %> </div>
	<div>	<%= link_to "details", list_path(list), class: "btn btn-primary"%>
	</div>
    <div>	<%= link_to "add bookmark", new_list_bookmark_path(list), class: "btn btn-primary"%>
	</div>
	<% end %>
	
	<div><%= link_to "create new list", new_list_path, class: "btn btn-primary"%></div>
	
	

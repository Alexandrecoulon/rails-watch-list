<h1>All Lists</h1>
<ul>
  <% @lists.each do |list| %>
	<li>
	<%= list.name %>
	<%= link_to "details", list_path(list), class: "btn btn-primary"%>
    <%= link_to "add bookmark", new_list_bookmark_path(list), class: "btn btn-primary"%>
	<% end %>
	</li>
	<%= link_to "create new list", new_list_path, class: "btn btn-primary"%>
</ul>
